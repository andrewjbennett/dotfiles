Config { font = "-misc-fixed-*-*-*-*-10-*-*-*-*-*-*-*"
        , bgColor = "black"
        , fgColor = "grey"
        , position = Top
        , lowerOnStart = True
        , commands = [
		Run Network "eth0" ["-L","0","-H","32","--normal","green","--high","red"] 10
		, Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
		, Run Memory ["-t","Mem: <usedratio>%"] 10
		, Run Swap [] 10
		, Run Com "uname" ["-s","-r"] "" 36000
		, Run Com "whoami" [] "" 36000
		, Run Com "hostname" [] "" 36000
		, Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
		, Run StdinReader
		, Run Network "ppp0" ["-t", "<rxbar>|<txbar>"] 10
		, Run Network "wlp3s0" ["-L","0","-H","32","--normal","green","--high","red"] 10
		, Run Com "/bin/bash" ["-c",  "~/.battery.sh"]  "batt" 10
		, Run Com "/bin/bash" ["-c", "~/.temp.sh"] "temp" 10
	]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%cpu% | %memory% | %swap% | %temp% | %wlp3s0% | %batt%  } %StdinReader% { %acpi% |  <fc=#ee9a00>%date%</fc> "
       }
}
